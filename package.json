{
  "name": "typescript-react-boilerplate",
  "version": "1.0.3",
  "description": "Simple typescript & React boilerplate",
  "scripts": {
    "test": "start-server-and-test test:storybook http-get://localhost:9009 test:jest",
    "test:jest": "jest",
    "test:storybook": "npm run build:storybook && serve -l 9009 dist-storybook",
    "test:update": "start-server-and-test test:storybook http-get://localhost:9009 test:jest:update",
    "test:jest:update": "jest -u",
    "build:app": "rm -Rf dist && npm run clear && npm run type:scss && webpack --config .buildConfig/webpack.production.ts",
    "build:storybook": "rm -Rf dist-storybook && npm run clear && npm run type:scss && build-storybook -o dist-storybook -s public -c ./.buildConfig/storybook",
    "dev:app": "webpack-dev-server --config .buildConfig/webpack.development.ts",
    "dev:storybook": "start-storybook -p 9009 -s public -c ./.buildConfig/storybook",
    "type:scss": "tsm src && tsm test",
    "watch:scss:app": "tsm src -w",
    "watch:scss:test": "tsm test -w",
    "clear": "clearb"
  },
  "repository": "https://github.com/mati-dev/typescript-react-boilerplate",
  "keywords": [
    "typescript",
    "react"
  ],
  "author": "Pablo Macias Moreira",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "bind-decorator": "^1.0.11",
    "classnames": "^2.2.6",
    "escape-string-regexp": "^4.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typed-scss-modules": "^1.3.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^25.2.3",
    "@types/jest-each": "^24.3.0",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/lodash": "^4.14.152",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/param-case": "^1.1.2",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-thunk": "^2.1.0",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-viewport": "^5.2.1",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.13",
    "@types/webpack-dev-server": "^3.11.0",
    "babel-loader": "^8.1.0",
    "clearb": "^0.1.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-each": "^26.0.1",
    "jest-image-snapshot": "^4.0.0",
    "jest-puppeteer": "^4.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "param-case": "^3.0.3",
    "puppeteer": "^3.1.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
